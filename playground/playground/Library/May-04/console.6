let strangerThing = function (initialValue) {

  let privateCounter = [initialValue]
  let n = Math.round(Math.random()*100)
  let counterUp = function () {
    privateCounter = privateCounter + 1
  }
  let counterDown = function () {
    privateCounter = privateCounter - 1
  }
  return { 
    'counterUp': counterUp,
    'counterDown': counterDown,
    // get counter () { return privateCounter },
    set counter (value) { 
      if (! isNaN(Number(value))) 
      { 
        privateCounter.push(Number(value))
      }
      console.log('privateCounter is now', privateCounter)
    },
    'id': n
  }

}

