let strangerThing = function (letter) {

  let song = [
      { 'line': 'a partridge in a pear tree', 'dayNumber': function () { return 1 }},
      { 'line': 'two turtle doves', 'dayNumber': function () { return 2 }},
      { 'line': 'three French hens', 'dayNumber': function () { return 3 }},
      { 'line': 'four calling birds', 'dayNumber': function () { return 4 }},
      { 'line': 'five golden rings', 'dayNumber': function () { return 5 }},
      { 'line': 'six geese a-laying', 'dayNumber': function () { return 6 }},
      { 'line': 'seven swans a-swimming', 'dayNumber': function () { return 7 }},
      { 'line': 'eight maids a-milking', 'dayNumber': function () { return 8 }},
      { 'line': 'nine ladies dancing', 'dayNumber': function () { return 9 }},
      { 'line': 'ten lords a-leaping', 'dayNumber': function () { return 10 }},
      { 'line': 'eleven pipers piping', 'dayNumber': function () { return 11 }},
      { 'line': 'twelve drummers drumming', 'dayNumber': function () { return 12 }}
  ]

  let n = Math.round(Math.random()*10)

  let myFunction = function () {
    console.log('My random number is', n)
    return song.filter ( (element) => element.line.includes(letter) ).map (
      (element) => element.line
    )
  }

  return { 'function': myFunction }

}

let myLettersOfInterest = ['a','b','c']
let myFunctionsOfInterest = myLettersOfInterest.map ( letter => strangerThing(letter) )
console.log('My functions of interest:', myFunctionsOfInterest)

for (aFunction of myFunctionsOfInterest) {
  console.log( aFunction.function() )
}
